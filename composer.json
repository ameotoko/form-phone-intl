{
    "name": "ameotoko/form-phone-intl",
    "description": "A phone number widget for Contao CMS",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Andrey Vinichenko"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "symfony/http-client": "^5.4 || ^6.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Ameotoko\\PhoneIntl\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Ameotoko\\PhoneIntl\\ContaoManager\\Plugin"
    }
}
